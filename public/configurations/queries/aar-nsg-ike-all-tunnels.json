{
    "id": "aar-nsg-ike-all-tunnels",
    "title": "TBD",
    "service": "elasticsearch",
    "query": {
        "index": "{{index:nuage_ike_tunnel_status}}",
        "type": "{{type:nuage_doc_type}}",
        "body": {
            "size": 0,
            "query": {
                "bool": {
                    "must": [
                        {
                            "range": {
                                "timestamp": {
                                    "gte": "{{startTime:now-15m}}",
                                    "lte": "{{endTime:now}}",
                                    "format": "epoch_millis"
                                }
                            }
                        },
                        {
                            "term": {
                                "SourceNSG": "{{snsg}}"
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "tunnel": {
                    "terms": {
                        "field": "TunnelName"
                    }
                }
            }
        }
    }
}

