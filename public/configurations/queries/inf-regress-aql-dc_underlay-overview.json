{
    "id": "inf-regress-aql-dc_underlay-overview",
    "title": "",
    "service": "elasticsearch",
    "tabify": "aqlSimpleText",
    "query": {
        "index": "nuage_aql",
        "type": "{{type:nuage_doc_type}}",
        "body": {
            "size": 0,
            "query": {
                "bool": {
                    "must": [
                        {
                            "term": {
                                "Area": "dc_underlay"
                            }
                        },
                        {
                            "term": {
                                "branch": "{{branch}}"
                            }
                        },
                        {
                            "range": {
                                "timestamp": {
                                    "gte": "{{startTime:now-1d}}",
                                    "lte": "{{endTime:now}}",
                                    "format": "epoch_millis"
                                }
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "area": {
                    "terms": {
                        "field": "Area"
                    },
                    "aggs": {
                        "suites": {
                            "terms": {
                                "field": "TestSuiteName",
                                "size": 100
                            },
                            "aggs": {
                                "run_count": {
                                    "filters": {
                                        "filters": {
                                            "NotRun": {
                                                "term": {
                                                    "TimesRun": 0
                                                }
                                            },
                                            "Total": {
                                                "exists": {
                                                    "field": "TimesRun"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}