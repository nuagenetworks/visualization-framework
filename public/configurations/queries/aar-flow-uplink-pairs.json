{
    "id": "aar-flow-uplink-pairs",
    "title": "TBD",
    "service": "elasticsearch",
    "query": {
        "index": "{{index:nuage_dpi_probestats}}",
        "type": "{{type:nuage_doc_type}}",
        "body": {
            "size": 0,
            "query": {
                "bool": {
                    "must": [
                        {
                            "range": {
                                "timestamp": {
                                    "gte": "{{startTime:now-24h}}",
                                    "lte": "{{endTime:now}}",
                                    "format": "epoch_millis"
                                }
                            }
                        },
                        {
                            "term": {
                                "SourceNSG": "{{snsg}}"
                            }
                        },
                        {
                            "term": {
                                "DestinationNSG": "{{dnsg}}"
                            }
                        },
                        {
                            "term": {
                                "APPGroup": "{{appGroup}}"
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "srcUplink": {
                    "terms": {
                        "field": "SrcUplink"
                    },
                    "aggs": {
                        "dstUplink": {
                            "terms": {
                                "field": "DstUplink"
                            }
                        }
                    }
                }
            }
        }
    }
}

