{
    "id":"aar-nsg-ike-tunnel-packet-traffic-linechart",
    "title":"TBD",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_ike_stats}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "sort":{
              "timestamp":"desc"
            },
	    "size":0,
            "query":{
                  "bool":{
                    "must":[
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-15m}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        },
                        {
                            "term": {
                                "SourceNSG": "{{snsg}}"
                            }
                        }
                    ]
                }
            },
            "aggs":{
        "date_histo":{
            "date_histogram": {
                        "field": "timestamp",
                        "interval": "1m"
                },
                "aggs":{
                    "tunnel":{
                        "terms":{
                            "field":"TunnelName"
                        },
                        "aggs":{
                            "{{ikePacketTypeColumn}}":{
                                "sum":{
                                    "field":"{{ikePacketTypeColumn}}"
                                    }
                        },
                            "throughput":{
                                "sum":{
                                    "script":{
                                        "inline":"doc['{{ikeBytesTypeColumn}}'].value/60.0"}
                                    }
                        }
                    }
                }
            }
        }
    }
}
}
}

