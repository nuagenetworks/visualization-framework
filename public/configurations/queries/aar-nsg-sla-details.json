{
    "id":"aar-nsg-sla-details",
    "title":"Get NSG sla violation details",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_dpi_slastats}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "query":{
                "bool":{
                    "must":[
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{cellstartTime}}",
                                    "lte":"{{cellendTime}}"
                                }
                            }
                        },
                        {
                            "term":{
                                "Application":"{{appName}}"
                            }
                        },
                        {
                            "term":{
                                "SourceNSG":"{{snsg:ovs-1}}"
                            }
                        }
                    ]
                }
            }
        }
    }
}
