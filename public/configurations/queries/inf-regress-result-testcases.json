{
    "id":"inf-regress-result-testcases",
    "title":"TBD",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_regress}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "size":0,
            "query": {
                "bool": {
                    "filter": [
                        {"wildcard": { "version.vrs": "{{vrsVersion}}" }},
                        {"wildcard": { "version.vsc": "{{vscVersion}}" }},
                        {"wildcard": { "version.vsd": "{{vsdVersion}}" }}
                    ],
                    "must_not": [
                        {"term": { "testcase": "" }},
                        {
                            "bool": {
                                "minimum_should_match": 1,
                                "should": [
                                    {
                                        "match_phrase": {
                                            "testsuite": "Sanity"
                                        }
                                    },
                                    {
                                        "match_phrase": {
                                            "testsuite": "initial"
                                        }
                                    },
                                    {
                                        "match_phrase": {
                                            "testsuite": "final"
                                        }
                                    },
                                    {
                                        "match_phrase": {
                                            "testsuite": "finished"
                                        }
                                    }
                                ]
                            }
                        }
                    ],
                    "must": [
                        {
                            "range":{
                                "time_regression_submitted":{
                                    "gte":"{{startTime:now-24h}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "buildnumber": {
                    "terms": {
                        "field": "build_number",
                        "size": 10000,
                        "order": {
                            "_count": "desc"
                        }
                    },
                    "aggs": {
                        "testcase": {
                            "terms": {
                                "field": "testcase",
                                "size": 10000,
                                "order": {
                                    "_count": "desc"
                                }
                            },
                            "aggs": {
                                "pass": {
                                    "sum_bucket": {
                                        "buckets_path": "pass-bucket>_count"
                                    }
                                },
                                "fail": {
                                    "sum_bucket": {
                                        "buckets_path": "fail-bucket>_count"
                                    }
                                },
                                "notrun": {
                                    "sum_bucket": {
                                        "buckets_path": "notrun-bucket>_count"
                                    }
                                },
                                "pass-bucket": {
                                    "filters": {
                                        "filters": {
                                            "pass-filter": {
                                                "match": {
                                                    "result": 2
                                                }
                                            }
                                        }
                                    }
                                },
                                "fail-bucket": {
                                    "filters": {
                                        "filters": {
                                            "fail-filter": {
                                                "match": {
                                                    "result": 1
                                                }
                                            }
                                        }
                                    }
                                },
                                "notrun-bucket": {
                                    "filters": {
                                        "filters": {
                                            "notrun-filter": {
                                                "match": {
                                                    "result": 0
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}

