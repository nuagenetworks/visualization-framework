{
  "id": "inf-regress-area-testsuite-table",
  "graph": "Table",
  "title": "{{testSuite}} run for date: {{testsuiteTime:call('epoch_to_date')}}",
  "description": "Colors of the result depict these outcomes.\n\nGray -> Skip\nRed -> Fail\nGreen -> Passed.",
  "author": "Ronak Shah",
  "data": {
    "columns": [
      {
        "column": "job_id",
        "label": "Job Id",
        "tooltip":{"column":"regression_link"}
      },
      {
        "column": "testcase",
        "label": "Test Case"
      },
      {
        "column": "platform",
        "label": "Platform",
        "selection":true
      },
      {
        "column": "runLevel",
        "label": "Run Level",
        "selection":true
      },
      {
        "column": "result",
        "label": "Result",
        "selection":true,
        "colors": {
          "0": "gray",
          "1": "red",
          "2": "green"
        }
      }
    ]
  },
  "listeners": [
    {
      "params":{
        "testCase":"testcase"
      }
    }
  ],
  "query": "inf-regress-area-testsuite-table"
}