{
    "id": "inf-regress-area-testsuite-heatmap",
    "graph": "HeatmapGraph",
    "title": "",
    "description": "Probe Status Heatmap for all IKE Tunnels sent at 2-minute interval.\n\nGREEN: Probe is UP\nRED: Probe is DOWN\nGRAY: Probe has been stopped because the IKE Tunnel is DOWN\nBLUE: Probe is in HOLD DOWN\nWHITE: There is no data for the probe\nYELLOW: Probe has gone through multiple state changes",
    "author": "Bharat Mukheja",
    "creationDate": "3/6/2018",
    "data": {
        "xAlign": true,
        "xColumn": "date_histo",
        "xLabel": "Time",
        "yColumn": "testsuite",
        "yTickGrid": false,
        "brush":8,
        "brushArea":3,
        "yLabel": "TestSuite",
        "legendColumn": "Runs",
        "colorColumn": "Runs",
        "heatmapColor": {
            "false": "white",
            "true": "lightBlue"
        },
        "legend": {
            "show": true,
            "orientation": "horizontal",
            "circleSize": 4,
            "labelOffset": 2
        },
        "tooltip": [
            { "column": "date_histo", "label": "Timestamp", "timeFormat": "%b %d"},
            { "column": "testsuite", "label": "Test Suite" },
            { "column": "Runs", "label": "Was Run?" }
        ]
   },
   "listeners": [
        {
            "params": {
                "testsuiteTime": "date_histo",
                "testSuite":"testsuite"
            }
        }
    ],
    "query": "inf-regress-area-testsuite-heatmap"
}
