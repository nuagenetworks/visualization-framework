{
    "id": "inf-regress-area-testsuite-heatmap",
    "graph": "HeatmapGraph",
    "title": "",
    "description": "",
    "author": "Bharat Mukheja",
    "creationDate": "3/6/2018",
    "data": {
        "xAlign": true,
        "xColumn": "date_histo",
        "xLabel": "Time",
        "yColumn": "testsuite",
        "yTickGrid": false,
        "brush":8,
        "brushArea":3,
        "yLabel": "TestSuite",
        "legendColumn": "result",
        "colorColumn": "result",
        "heatmapColor": {
            "SKIP": "gray",
            "FAIL": "red",
            "PASS": "green",
            "": "white"
        },
        "legend": {
            "show": true,
            "orientation": "horizontal",
            "circleSize": 4,
            "labelOffset": 2
        },
        "tooltip": [
            { "column": "date_histo", "label": "Timestamp", "timeFormat": "%b %d"},
            { "column": "testsuite", "label": "Test Suite" },
            { "column": "build_number", "label": "Build Number" },
            { "column": "result", "label": "Result"}
        ],
        "tabifyOptions":{
            "suiteAreasFile": "../files/testsuite_areas.json",
            "aql_area":"{{testsuiteArea}}"
        }
   },
   "listeners": [
        {
            "params": {
                "testsuiteTime": "date_histo",
                "testSuite":"testsuite"
            }
        }
    ],
    "query": "inf-regress-area-testsuite-heatmap"
}
