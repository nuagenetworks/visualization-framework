             "#7da3f7",
             "#fec26a",
             "#e78ac3",
             "#f79e99"
        ],
        "margin": {
            "top": 20,
            "bottom": 40,
            "left": 80,
            "right": 40
        },
        "xTickGrid": true,
        "yTickGrid": false,
        "xTickFormat": ".2s",
