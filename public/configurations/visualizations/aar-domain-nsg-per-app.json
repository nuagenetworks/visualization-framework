{
    "id": "aar-domain-nsg-per-app",
    "graph": "BarGraph",
    "title": "Data Usage per NSG for {{app}} App",
    "description": "Bar graph displays the data usage per NSG location for a selected application",
    "author": "Bharat Mukheja",
    "creationDate": "10/15/2017",
    "data": {
        "colors": [
            "#7da3f7",
            "#fec26a",
