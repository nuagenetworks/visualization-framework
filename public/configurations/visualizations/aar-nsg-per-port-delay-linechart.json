{
    "id": "aar-nsg-per-port-delay-linechart",
    "graph": "MultiLineGraph",
    "title": "SLA Delay for {{app}}",
    "description": "Line graph displays the Latency measurements (ms) for the application based on the APM probes. The green line is the applicationâ€™s configured value of Latency SLA in VSD",
    "author": "Bharat Mukheja",
    "creationDate": "11/17/2017",
    "data": {
        "dateHistogram": true,
        "xColumn": "ts",
        "yColumn": ["Delay"],
        "yTickFormat": ".2s",
        "xLabel": "Time",
        "yLabel": "Delay (ms)",
        "linesColumn":["Delay"],
        "stroke": {
          "color": "#f76159",
          "width": "2px"
        },
        "defaultY": {
            "source": "config-data",
            "column": "oneWayDelay"
        },
        "colors": [
            "#ff0000",
            "#000000"
        ],
        "legend": {
            "orientation": "horizontal",
            "show": true,
            "circleSize": 5,
            "labelOffset": 5
        },
        "tooltip": [
            { "column": "yColumn","label":"Actual Delay" }
        ]
    },
    "query": {
        "data": "aar-nsg-per-port-delay-linechart",
        "config-data": "aar-nsg-app-config"
    }
}
