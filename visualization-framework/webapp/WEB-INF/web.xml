<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <mime-mapping>
        <extension>sj</extension>
        <mime-type>text/javascript</mime-type>
    </mime-mapping>
    <mime-mapping>
        <extension>cib</extension>
        <mime-type>text/plain</mime-type>
    </mime-mapping>
    <mime-mapping>
        <extension>manifest</extension>
        <mime-type>text/cache-manifest</mime-type>
    </mime-mapping>
    <mime-mapping>
        <extension>plist</extension>
        <mime-type>text/plain</mime-type>
    </mime-mapping>
    <session-config>
        <cookie-config>
            <http-only>true</http-only>
            <secure>true</secure>
        </cookie-config>
        <tracking-mode>COOKIE</tracking-mode>
    </session-config>
   <servlet>
        <servlet-name>dashboards</servlet-name>
        <jsp-file>index.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>dashboards</servlet-name>
        <url-pattern>dashboards/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>visualizations</servlet-name>
        <jsp-file>index.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>visualizations</servlet-name>
        <url-pattern>visualizations/*</url-pattern>
    </servlet-mapping>
</web-app>
