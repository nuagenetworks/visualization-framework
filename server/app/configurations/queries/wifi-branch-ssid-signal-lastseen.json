{
    "id":"wifi-branch-ssid-signal-lastseen",
    "title":"TBD",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_wifi}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "query":{
                "bool":{
                    "must":[
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"now-1m",
                                    "lte":"now",
                                    "format":"epoch_millis"
                                }
                            }
                        },
                        {
                            "term": {
                                "ssid-name": "{{ssid}}"
                            }
                        },
                        {
                            "term":{
                                "enterpriseName":"{{enterpriseName:wifi_enterprise}}"
                            }
                        },
                        {
                            "term":{
                                "nsg":"{{nsg:wifi_nsg}}"
                            }
                        }
                    ]
                }
            }
        }
    }
}
