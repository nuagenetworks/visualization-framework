{
    "id":"aar-domain-top5-apmg",
    "title":"TBD",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_dpi_flowstats}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "size":0,
            "query":{
                "bool":{
                    "must":[
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-24h}}",
                                    "lte":"{{endTimeTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        },
                        {
                            "term":{
                                "EnterpriseName":"{{enterpriseName:test_organization}}"
                            }
                        },
                        {
                            "term":{
                                "{{domainType:Domain}}":"{{domainName:Domain1}}"
                            }
                        }
                    ]
                }
            },
            "aggs":{
                "APMGroup":{
                    "terms":{
                        "field":"APMGroup",
                        "size":5,
                        "order":{
                            "Sum of MB":"desc"
                        }
                    },
                    "aggs":{
                        "Sum of MB":{
                            "sum":{
                                "field":"TotalMB"
                            }
                        }
                    }
                }
            }
        }
    }
}
