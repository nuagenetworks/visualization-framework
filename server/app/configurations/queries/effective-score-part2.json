{
    "id":"effective-score-part2",
    "title":"TBD",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_dpi_flowstats}}",
        "type":"{{type:nuage_doc_type}}",
        "size":0,
        "query":{
            "bool":{
                "must":[
                    {
                        "range":{
                            "timestamp":{
                                "gte":"now-1y",
                                "lte":"now",
                                "format":"epoch_millis"
                            }
                        }
                    },
                    {
                        "term":{
                            "SlaTransition":"True"
                        }
                    },
                    {
                        "term":{
                            "EnterpriseName":"test_org"
                        }
                    }
                ]
            }
        },
        "aggs":{
            "Application":{
                "cardinality":{
                    "field":"Application"
                }
            }
        }
    }
}
