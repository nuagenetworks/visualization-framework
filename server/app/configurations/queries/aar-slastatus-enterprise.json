{
    "id":"slaStatus-PieChart-Enterprise",
    "title":"SLA Status per Enterprise",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_dpi_flowstats}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "size":0,
            "query":{
                "bool":{
                    "must":[
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-24h}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        },
                        {
                            "term":{
                                "EnterpriseName":"{{enterpriseName:test_org}}"
                            }
                        }
                    ]
                }
            },
            "aggs":{
                "slastatus":{
                    "terms":{
                        "field":"SlaStatus"
                    }
                }
            }
        }
    }
}

