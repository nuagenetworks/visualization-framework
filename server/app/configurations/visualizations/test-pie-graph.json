            "show": true,
            "orientation": "horizontal",
            "circleSize": 4,
            "labelOffset": 2
        }
    },
    "listeners": [{
      "redirect": "/dashboards/dateHistogramExample",
      "params": {
          "app": "Application"
      }
    }],
    "query": "test-pie-graph"
