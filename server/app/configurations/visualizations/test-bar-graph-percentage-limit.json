        "tooltip": [
            { "column": "Application", "label": "Application" },
            { "column": "value", "format": ",.2s"}
        ]
    },
    "listeners": [
        {
            "redirect": "/dashboards/aarEnterpriseDetail",
            "params": {
                "app": "Application"
